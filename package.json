{
  "name": "@studiokloek/ts-core-lib",
  "version": "2.0.0-1",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:watch": "babel src --watch --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "np": {
    "yarn": false,
    "tests": false
  },
  "engines": {
    "node": ">=12.20.1",
    "npm": ">=7.5.6"
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.10",
    "@capacitor/core": "^2.4.7",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^3.6.0",
    "howler": "^2.2.1",
    "lodash-decorators": "^6.0.1",
    "lodash-es": "^4.17.21",
    "pixi-spine": "^2.1.11",
    "pixi.js-legacy": "^6.0.0",
    "pubsub-js": "^1.9.3",
    "random-js": "^2.1.0",
    "slugify": "^1.4.7",
    "stats.js": "^0.17.0",
    "superagent": "^6.1.0",
    "ts-events": "^3.4.0",
    "core-js": "^3.9.1",
    "ts-mixer": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/fontfaceobserver": "0.0.6",
    "@types/howler": "^2.2.1",
    "@types/lodash-es": "^4.17.4",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-unicorn": "^28.0.2",
    "np": "^7.4.0",
    "npm": "7.5.6",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 60",
    "last 2 Safari versions",
    "not Safari < 10.1",
    "last 2 iOS versions",
    "not iOS < 10.3",
    "last 2 Firefox versions",
    "not Firefox < 54",
    "last 2 Edge versions",
    "not Edge < 15"
  ]
}
