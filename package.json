{
  "name": "@studiokloek/ts-core-lib",
  "version": "1.15.8",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:watch": "babel src --watch --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.10.5",
    "@capacitor/core": "^2.4.0",
    "bowser": "^2.10.0",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^3.4.2",
    "howler": "^2.2.0",
    "lodash-es": "^4.17.15",
    "pixi-spine": "^2.1.9",
    "pixi.js-legacy": "^5.3.2",
    "pubsub-js": "^1.8.0",
    "random-js": "^2.1.0",
    "slugify": "^1.4.5",
    "stats.js": "^0.17.0",
    "superagent": "^5.3.1",
    "ts-events": "^3.4.0",
    "ts-mixer": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/fontfaceobserver": "0.0.6",
    "@types/howler": "^2.2.1",
    "@types/lodash-es": "^4.17.3",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "core-js": "^3.6.5",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^21.0.0",
    "lodash-decorators": "^6.0.1",
    "np": "^6.3.2",
    "npm": "^6.14.7",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 60",
    "last 2 Safari versions",
    "not Safari < 10.1",
    "last 2 iOS versions",
    "not iOS < 10.3",
    "last 2 Firefox versions",
    "not Firefox < 54",
    "last 2 Edge versions",
    "not Edge < 15"
  ]
}
