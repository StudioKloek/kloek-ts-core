{
  "name": "@studiokloek/ts-core-lib",
  "version": "2.0.0-15",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:script",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc"
  },
  "np": {
    "yarn": false,
    "tests": false
  },
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "lib/*"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.7",
    "@types/fontfaceobserver": "0.0.6",
    "@types/howler": "^2.2.2",
    "@types/lodash-es": "^4.17.4",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.11",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^3.6.1",
    "howler": "^2.2.1",
    "lodash-decorators-esm": "^6.1.1",
    "lodash-es": "^4.17.21",
    "pixi-spine": "^3.0.3",
    "pixi.js-legacy": "^6.0.4",
    "pubsub-js": "^1.9.3",
    "random-js": "^2.1.0",
    "ress": "^4.0.0",
    "slugify": "^1.5.3",
    "superagent": "^6.1.0",
    "ts-events": "^3.4.0",
    "ts-mixer": "^5.4.1",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "np": "^7.5.0",
    "npm": "7.15.0",
    "prettier": "^2.3.0",
    "prettier-package-json": "^2.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ]
}
