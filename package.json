{
  "name": "@studiokloek/ts-core-lib",
  "version": "2.0.0-5",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:watch": "babel src --watch --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "np": {
    "yarn": false,
    "tests": false
  },
  "engines": {
    "node": ">=12.20.1",
    "npm": ">=7.5.6"
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.13.17",
    "@capacitor/core": "^2.4.7",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^3.6.1",
    "howler": "^2.2.1",
    "lodash-decorators": "^6.0.1",
    "lodash-es": "^4.17.21",
    "pixi-spine": "^3.0.0",
    "pixi.js-legacy": "^6.0.2",
    "pubsub-js": "^1.9.3",
    "random-js": "^2.1.0",
    "slugify": "^1.5.0",
    "superagent": "^6.1.0",
    "ts-events": "^3.4.0",
    "ts-mixer": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/fontfaceobserver": "0.0.6",
    "@types/howler": "^2.2.2",
    "@types/lodash-es": "^4.17.4",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-polyfill-corejs3": "^0.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "np": "^7.5.0",
    "npm": "7.11.1",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ]
}
