{
  "name": "@studiokloek/ts-core-lib",
  "version": "1.6.9",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:watch": "babel src --watch --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/fontfaceobserver": "0.0.6",
    "@types/gsap": "^1.20.2",
    "@types/howler": "^2.1.2",
    "@types/lodash-es": "^4.17.3",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^13.0.0",
    "lodash-decorators": "^6.0.1",
    "np": "^5.0.1",
    "prettier": "^1.19.1",
    "prettier-package-json": "^2.1.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@babel/helpers": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@capacitor/core": "^1.3.0",
    "bowser": "^2.7.0",
    "core-js": "^3.4.2",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^2.1.3",
    "gsap-then": "^3.0.0",
    "howler": "^2.1.2",
    "lodash-es": "^4.17.15",
    "npm": "^6.13.1",
    "pixi-spine": "2.1.4",
    "pixi.js-legacy": "^5.2.0",
    "pubsub-js": "^1.7.0",
    "random-js": "^2.1.0",
    "regenerator-runtime": "^0.13.3",
    "stats.js": "^0.17.0",
    "superagent": "^5.1.1",
    "ts-events": "^3.3.1",
    "ts-mixer": "^4.0.0"
  },
  "browserslist": [
    "> 0.5% in NL",
    "not ie 11",
    "not Safari < 10"
  ]
}
